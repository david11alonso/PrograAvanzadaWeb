@model FrontEnd.Models.Departamento

@{
    ViewData["Title"] = "Delete";
}

<h2 class="alert alert-warning alert-dismissible fade show " style="text-align: center;">  <strong>Atencion!</strong> ¿Esta seguro de eliminar este departamento?</h2>
<hr />

<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="DepartamentoId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
    </form>
</div>

<br />
<section class="content">
    <div class="container-fluid">

        <div>
            <a asp-action="Index">Volver al Menú Principal</a>
        </div>
    </div>
</section>
